{
    "name": "sindll-tech/think-jwt",
    "description": "The ThinkPHP jwt Package",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "wisoch",
            "email": "wisoch@163.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "think\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "require": {
        "php": ">=8.1.0",
        "topthink/framework": "^6.1|^8.0",
        "firebase/php-jwt": "^6.11"
    },
    "extra": {
        "think": {
            "services": [
                "think\\service\\JWTService"
            ],
            "config": {
                "jwt": "src/config/jwt.php"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
